using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace BusinessObjectLibrary
{
    public class ActionTreeObject
    {
        public string A_ID { get; set; }
        public string A_ID_E { get; set; }
        public string A_OWNER { get; set; }
        public string A_SHORT_DESCRIPTION { get; set; }
        public string A_LONG_DESCRIPTION { get; set; }
        public string A_PLANNED_START { get; set; }
        public string A_START_DATE { get; set; }
        public string A_PLANNED_END { get; set; }
        public string A_END_DATE { get; set; }
        public string A_FORECAST_ENDDATE { get; set; }
        public string A_STATUS { get; set; }
        public string A_PARENT { get; set; }
        public string A_ACCOMPLISHMENT { get; set; }
        public string A_STATUS_REPORT { get; set; }
        public string A_WEIGHT { get; set; }
        public string A_E_ACCOMPLISHMENTS { get; set; }
        public string text { get; set; } // Tree Model parameter
        public string HASCHILD { get; set; }
        public bool leaf { get; set; }
        public bool expanded { get; set; }
        public List<ActionTreeObject> filhos { get; set; }

        public ActionTreeObject()
        {
            this.filhos = new List<ActionTreeObject>();
        }
    }
}
